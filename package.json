{
  "name": "trading-ai-agent",
  "version": "1.0.0",
  "description": "AI Trading Agent using Game Engine SDK and Enzyme Protocol SDK with Multi-Position Management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test:quickstart": "node test-quickstart.js",
    "test:legacy": "node test-script.js",
    "test:multi": "node test-script-multiposition.js",
    "test:quick": "node test-individual.js quick",
    "test:conflict": "node test-individual.js conflict",
    "test:connection": "node test-individual.js connection",
    "test:single": "node test-individual.js single",
    "test:multiple": "node test-individual.js multiple",
    "test:different": "node test-individual.js different",
    "test:risk": "node test-individual.js risk",
    "test:limit": "node test-individual.js limit",
    "test:status": "node test-individual.js status",
    "test:monitor": "node test-individual.js monitor",
    "test:menu": "node test-individual.js",
    "test:all": "npm run test:legacy && npm run test:multi",
    "test:demo": "npm run test:connection && npm run test:single && npm run test:status",
    "test:signal-listener": "npm run build && node test-signal-listener.js"
  },
  "keywords": [
    "trading",
    "ai-agent",
    "game-engine",
    "enzyme-protocol",
    "defi",
    "multi-position",
    "risk-management"
  ],
  "author": "Trading Minds",
  "license": "MIT",
  "dependencies": {
    "@enzymefinance/sdk": "^1.0.0",
    "@types/mongodb": "^4.0.7",
    "@virtuals-protocol/game": "^0.1.14",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "mongodb": "^6.17.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
